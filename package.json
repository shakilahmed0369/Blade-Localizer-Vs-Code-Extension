{
  "name": "laravelbladelocalizer",
  "displayName": "Laravel Blade Localizer",
  "description": "A vs code extension to wrap blade static texts in laravel localization syntax.",
  "publisher": "ShakilAhmed",
  "icon": "images/icon.png",
  "version": "2.1.0",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:laravel.blade.localizer"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravel.blade.localizer",
        "title": "Laravel: Localize blade static texts"
      }
    ],
    "configuration": [
      
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shakilahmed0369/Wsus-Localizer-Vs-Code-Extension.git"
  }
}
