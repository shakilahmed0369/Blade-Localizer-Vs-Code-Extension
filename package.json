{
  "name": "wsuslaravellocalizer",
  "displayName": "wus laravel localizer",
  "description": "A vs code extension to wrap blade static texts in laravel localization syntax.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wsus.localizer",
        "title": "Wsus: Laravel Localizer"
      }
    ] 
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.33"
  }
}
